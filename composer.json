{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "api-platform/core": "^2.7",
    "composer/package-versions-deprecated": "1.11.99.1",
    "creof/doctrine2-spatial": "^1.2",
    "doctrine/annotations": "^2.0",
    "doctrine/doctrine-bundle": "^2.11",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^2.16",
    "geocoder-php/arcgis-online-provider": "^4.4",
    "geocoder-php/free-geoip-provider": "^4.4",
    "geocoder-php/geo-plugin-provider": "^4.2",
    "geocoder-php/google-maps-provider": "^4.7",
    "google/apiclient": "^2.14",
    "google/auth": "^1.9",
    "guzzlehttp/guzzle": "^7.8",
    "http-interop/http-factory-guzzle": "*",
    "imagine/imagine": "^1.2",
    "ipinfo/ipinfo": "^2.1",
    "kreait/firebase-bundle": "^3.1",
    "kreait/firebase-php": "^5.25",
    "lexik/jwt-authentication-bundle": "^2.19",
    "nelmio/cors-bundle": "^2.1",
    "patreon/patreon": "^1.0",
    "php-ffmpeg/php-ffmpeg": "^1.1",
    "php-http/message": "^1.16",
    "phpdocumentor/reflection-docblock": "^5.2",
    "psr/http-factory": "^1.0",
    "statickidz/php-google-translate-free": "^1.2.1",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "5.4.*",
    "symfony/cache": "5.4.*",
    "symfony/console": "5.4.*",
    "symfony/css-selector": "5.4.*",
    "symfony/dotenv": "5.4.*",
    "symfony/expression-language": "5.4.*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "5.4.*",
    "symfony/google-mailer": "5.4.*",
    "symfony/http-client": "^5.4",
    "symfony/mailer": "5.4.*",
    "symfony/mime": "5.4.*",
    "symfony/monolog-bundle": "^3.6",
    "symfony/phpunit-bridge": "^5.2",
    "symfony/property-access": "5.4.*",
    "symfony/property-info": "5.4.*",
    "symfony/proxy-manager-bridge": "5.4.*",
    "symfony/runtime": "5.4.*",
    "symfony/security-bundle": "5.4.*",
    "symfony/security-core": "5.4.*",
    "symfony/serializer": "5.4.*",
    "symfony/translation-contracts": "^2.5",
    "symfony/twig-bundle": "5.4.*",
    "symfony/uid": "5.4.*",
    "symfony/validator": "5.4.*",
    "symfony/yaml": "5.4.*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^3.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.4.*"
    },
    "public_dir": "/var/www/vhosts/frikiradar.com/api.frikiradar.com/public/"
  },
  "require-dev": {
    "symfony/browser-kit": "5.4.*",
    "symfony/maker-bundle": "^1.50",
    "symfony/stopwatch": "^5.2",
    "symfony/web-profiler-bundle": "^5.2"
  }
}
