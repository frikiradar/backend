{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.1.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "creof/doctrine2-spatial": "^1.2",
    "doctrine/doctrine-bundle": "^1.9",
    "doctrine/orm": "^2.6",
    "friendsofsymfony/rest-bundle": "^2.5",
    "geocoder-php/free-geoip-provider": "^4.1",
    "geocoder-php/geo-plugin-provider": "^4.0",
    "geocoder-php/google-maps-provider": "^4.3",
    "imagine/imagine": "^1.2",
    "jms/serializer-bundle": "^3.0",
    "kreait/firebase-bundle": "^1.0",
    "lcobucci/jwt": "^3.2",
    "lexik/jwt-authentication-bundle": "^2.6",
    "nelmio/api-doc-bundle": "^3.3",
    "nelmio/cors-bundle": "^1.5",
    "php-http/guzzle6-adapter": "^2.0",
    "php-http/message": "^1.7",
    "sensio/framework-extra-bundle": "^5.2",
    "symfony/asset": "4.1.*",
    "symfony/console": "4.2.*",
    "symfony/dotenv": "4.2.*",
    "symfony/expression-language": "*",
    "symfony/flex": "^1.1",
    "symfony/form": "*",
    "symfony/framework-bundle": "4.2.*",
    "symfony/maker-bundle": "^1.11",
    "symfony/mercure-bundle": "^0.1.1",
    "symfony/messenger": "4.2.*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/orm-pack": "^1.0",
    "symfony/process": "*",
    "symfony/security-bundle": "*",
    "symfony/serializer-pack": "*",
    "symfony/swiftmailer-bundle": "^3.2",
    "symfony/twig-bundle": "4.2.*",
    "symfony/validator": "*",
    "symfony/web-link": "*",
    "symfony/yaml": "4.2.*",
    "willdurand/geocoder-bundle": "^5.0"
  },
  "require-dev": {
    "symfony/debug-pack": "*",
    "symfony/dotenv": "*",
    "symfony/maker-bundle": "^1.8",
    "symfony/profiler-pack": "*",
    "symfony/test-pack": "*",
    "symfony/web-server-bundle": "4.2.*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": "true",
      "require": "4.2.*"
    }
  }
}
